{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "SwaggerOptions": {
    "JsonRoute": "swagger/{documentName}/swagger.json",
    "Description": "MultiLingo API",
    "UIEndPoint": "v1/swagger.json"
  },
  "TokenConfigurations": {
    "Audience": "MultiLingoAudience",
    "Issuer": "MultiLingoConfigurations",
    "Seconds": 31533000
  },
    "AllowedHosts": "*",
    "ConnectionStrings": {
      "MultiLingoDb": "Server=localhost;Database=MultiLingo;Trusted_Connection=True;"
    }
  }
